<!--components/navi/navi.wxml-->
<view class='container'>
  <image class="navi-icon" bind:tap="onLeft"          
    src="{{latest?disLeftSrc:highLeftSrc}}" />
  <text class="title">{{title}}</text>
  <image class="navi-icon" disable="{{!first}}" bind:tap='onRight"
    src="{{first?'images/triangle.dis@right.png':'images/triangle@right.png'}}" />
</view>